{
    "compilerOptions": {
        "target": "ES2019",
        "module": "esnext",
        "baseUrl": ".",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strictPropertyInitialization": false,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "removeComments": false,
        "moduleResolution": "node"
    },
    "exclude": [
        "node_modules",
    ],
    "include": [
        "./**/*.ts",
    ],
}